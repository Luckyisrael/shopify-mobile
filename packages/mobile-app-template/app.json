{
  "expo": {
    "name": "ShopifyTestMobileApp",
    "slug": "ShopifyTestMobileApp",
    "version": "1.0.0",
    "scheme": "ShopifyTestMobileApp",
    "platforms": ["ios", "android"],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./src/assets/icon.png",
          "color": "#002d2d",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.luckyisrael.ShopifyTestMobileApp",
      "googleServicesFile": "./shopify-mobile-connector-firebase.json"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "21f76db7-f7fb-48ec-adf2-efb0dd6214b0"
      }
    }
  }
}
